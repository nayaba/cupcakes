<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th></th>
      <th>Item</th>
      <th>Price</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% items.forEach(function(i) { %>
      <tr>
        <td><img class="list-image" src="<%= i.images[0] %>" ></td>
        <td><%= i.name %></td>
        <td><%= i.price %></td>
        <!-- <td><a href="/items/<%= i._id %>">DETAILS</a></td> -->
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
